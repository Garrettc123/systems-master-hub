# Salesforce Configuration
SF_CLIENT_ID=your_connected_app_client_id_here
SF_CLIENT_SECRET=your_connected_app_client_secret_here
SF_USERNAME=your_salesforce_username@company.com
SF_PASSWORD=your_salesforce_password
SF_SECURITY_TOKEN=your_security_token
SF_LOGIN_URL=https://login.salesforce.com
SF_API_VERSION=v59.0

# Einstein Configuration
EINSTEIN_API_KEY=your_einstein_api_key
EINSTEIN_ENDPOINT=https://api.einstein.ai/v2

# Integration Services
REDIS_URL=redis://localhost:6379
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_PASSWORD=admin123
POSTGRES_URL=postgresql://sfuser:sfpass@localhost:5432/sf_integration

# API Configuration
API_PORT=3000
API_BASE_URL=https://your-domain.com
WEBHOOK_SECRET=your_webhook_secret_key_32_chars
JWT_SECRET=your_jwt_secret_key_32_chars

# AI Systems Integration
APEX_API_URL=https://apex-os.your-domain.com
APEX_API_KEY=your_apex_api_key

NWU_API_URL=https://nwu-protocol.your-domain.com
NWU_API_KEY=your_nwu_api_key

AI_OPS_API_URL=https://ai-ops-studio.your-domain.com
AI_OPS_API_KEY=your_ai_ops_api_key

MLOPS_API_URL=https://mlops.your-domain.com
MLOPS_API_KEY=your_mlops_api_key

AUTOHELIX_API_URL=https://autohelix.your-domain.com
AUTOHELIX_API_KEY=your_autohelix_api_key

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=false

# Feature Flags
ENABLE_EINSTEIN_LEAD_SCORING=true
ENABLE_CHURN_PREDICTION=true
ENABLE_OPPORTUNITY_INSIGHTS=true
ENABLE_REAL_TIME_SYNC=true
ENABLE_AUTO_PROVISIONING=true

# Security
ENCRYPTION_KEY=your_32_character_encryption_key_here
ALLOWED_ORIGINS=https://your-domain.com,https://app.your-domain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
